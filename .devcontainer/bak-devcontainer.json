                    "gitdoc.commitValidationLevel": "none",
                    "gitdoc.pullOnOpen": false,
                    "html.autoCreateQuotes": false,
                    "html.format.indentInnerHtml": true,
                    "html.suggest.html5": false,
                    "java.server.launchMode": "Standard",
                    "java.configuration.runtimes": [
                        {
