        "settings":         
        {            
                "editor.fontFamily": "CascadiaCode, 'Cascadia Code', Consolas, 'Courier New', monospace",
                "C_Cpp.updateChannel": "Insiders",
                "workbench.colorTheme": "Dracula",
                "latex-workshop.message.update.show": false,
