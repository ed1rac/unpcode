                    "C_Cpp.debugShortcut": false,
                    "C_Cpp.dimInactiveRegions": false,
                    "C_Cpp.doxygen.generateOnType": false,
                    "C_Cpp.enhancedColorization": "enabled",
                    "C_Cpp.errorSquiggles": "disabled",
                    "C_Cpp.formatting": "clangFormat",
